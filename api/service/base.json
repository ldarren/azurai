{
	"mod": {
		"web": {
			"mod": "@web",
			"host": "0.0.0.0",
			"cors": "*",
			"port": "$PORT"
		},
		"sse": {
			"mod": "@sse"
		},
		"jwt": {
			"mod": "auth"
		},
		"sys": {
			"mod": "@sys"
		},
		"timer": {
			"mod": "@timer"
		},
		"util":{
			"mod": "util"
		},
		"llm": {
			"mod": "llm",
			"vendor": "openai",
			"model": "gpt-3.5-turbo",
			"apiKey": "$mod_llm_api_key"
		},
		"hnsw": {
			"mod": "hnsw",
			"desc": "A useful tool to search internal company information",
			"dimensions": 1536,
			"space": "l2",
			"maxElements": 9999,
			"jsondb": "./db/db.json",
			"vectordb": "./db/vector.hnsw"
		},
		"serp": {
			"mod": "serp",
			"desc": "useful for when you need to find something on or summarize a webpage",
			"host": "$mod_serp_host",
			"params": {
				"key": "$mod_serp_api_key",
				"cx": "$mod_serp_engine",
				"safe": "active",
				"dateRestrict": "y1",
				"num": 10,
				"fields": "queries,items(title,link,snippet)"
			}
		},
		"md": {
			"mod": "md"
		},
		"gh": {
			"mod": "gh",
			"clientId": "$mod_gh_client_id",
			"clientSecret": "$mod_gh_client_secret"
		},
		"pg": {
			"mod": "pg_pool",
			"user": "$mod_pg_user",
			"password": "$mod_pg_password",
			"host": "$mod_pg_host",
			"port": "$mod_pg_port",
			"database": "azurai",
			"ssl": true,
			"max": 10,
			"connectionTimeoutMillis": 0,
			"idleTimeoutMillis": 10000
		}
	}
}
