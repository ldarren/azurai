{
	"mod": {
		"web": {
			"mod": "@web",
			"host": "0.0.0.0",
			"port": "$PORT"
		},
		"sse": {
			"mod": "@sse"
		},
		"auth": {
			"mod": "@basicAuth"
		},
		"sys": {
			"mod": "@sys"
		},
		"timer": {
			"mod": "@timer"
		},
		"util":{
			"mod": "util"
		},
		"llm": {
			"mod": "llm",
			"vendor": "openai",
			"model": "gpt-3.5-turbo",
			"apiKey": "$mod_llm_api-key"
		},
		"hnsw": {
			"mod": "hnsw",
			"desc": "A useful tool to search internal company information",
			"dimensions": 1536,
			"jsondb": "./db/db.json",
			"vectordb": "./db/vector.hnsw"
		},
		"serp": {
			"mod": "serp",
			"desc": "useful for when you need to find something on or summarize a webpage",
			"host": "$mod_serp_host",
			"params": {
				"key": "$mod_serp_api-key",
				"cx": "$mod_serp_engine",
				"safe": "active",
				"dateRestrict": "y1",
				"num": 1,
				"fields": "queries,items(title,link,snippet)"
			}
		}
	}
}
