{
	"routes": {
		"/1/ping": [
			[["web.output", "text/plain", "text"], "_.res", "pong"]
		],
		"/1/chat": [
			["web.bodyParser", "_.req", "_.body"],
			["llm.chat", "$.llm", "_.body.history", "_.body.overrides", "_.output"],
			[["web.output"], "_.res", "_.output"]
		],
		"/1/ask": [
			["web.bodyParser", "_.req", "_.body"],
			["llm.ask", "$.llm", "_.body.question", "_.body.overrides", "_.output"],
			[["web.output"], "_.res", "_.output"]
		],
		"/1/chain": [
			["web.bodyParser", "_.req", "_.body"],
			["util.push", "_.:tools", "$.hnsw"],
			["llm.rag", "$.llm", "_.body.question", "_.body.history", "_.:tools", "_.output"],
			[["web.output"], "_.res", "_.output"]
		],
		"/1/serp": [
			["web.bodyParser", "_.req", "_.body"],
			[["web.output"], "_.res", "_.output"]
		]
	}
}
