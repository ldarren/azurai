{
  "name": "azurai-ui",
  "version": "0.1.0",
  "description": "chat and train",
  "main": "index.js",
  "engines": {
    "npm": ">=9.0.0 <20.0.0",
    "node": ">=18.0.0 <20.0.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:local": "cp cfg/aui.dev.json cfg/aui.env.json && pico-client-build --bundle aui --env local && cp bin/index.dev.html bin/aui/index.html",
